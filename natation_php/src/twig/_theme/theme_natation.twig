{# Base natation Twig template #}
<!DOCTYPE html>
<html lang="fr"><head>
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
        <meta name="description" content="">
        <meta name="author" content="">
        <link rel="icon" href="https://getbootstrap.com/docs/3.3/favicon.ico">

        <title>{{page_title}}</title>

        <!-- Bootstrap core CSS -->
        <link href="{{site_base}}/ressources/css/bootstrap.min.css" rel="stylesheet">

        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <link href="{{site_base}}/ressources/css/ie10-viewport-bug-workaround.css" rel="stylesheet">

        <!-- FontAwesome style sheet -->
        <link href="{{site_base}}/ressources/css/fontawesome-all.min.css" rel="stylesheet">

        <!-- Custom styles for this template -->
        <link href="{{site_base}}/ressources/css/sticky-footer-navbar.css" rel="stylesheet">

        <!-- Custom style sheet -->
        <link href="{{site_base}}/ressources/css/custom.css" rel="stylesheet">

        <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
        <!--[if lt IE 9]><script src="../../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
        <script src="{{site_base}}/ressources/js/ie-emulation-modes-warning.js"></script>

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>

    <body>

        {%block banner%}
            <!-- Banner of the page -->
            <div class="banner fixed-top" id="page-banner">
                <div class="row">
                    <!--<div class="col-xs-6"><img src="Logo_natation.png"></img></div>
                    <div class="col-xs-6"><img src="Header_natation.png"></img></div>-->
                    <div class="col-sm-4 banner-left"></div>
                    <div class="col-sm-8 banner-right"></div>
                </div>
            </div>
        {%endblock%}

        {%block header%}
            <!-- Fixed navbar -->
            <nav class="navbar navbar-default navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="{{site_base}}">Project name</a>
                    </div>
                    <div id="navbar" class="collapse navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="{{site_base}}">Home</a></li>
                            <li><a href="#about">About</a></li>
                            <li><a href="#contact">Contact</a></li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Action</a></li>
                                    <li><a href="#">Another action</a></li>
                                    <li><a href="#">Something else here</a></li>
                                    <li role="separator" class="divider"></li>
                                    <li class="dropdown-header">Nav header</li>
                                    <li><a href="#">Separated link</a></li>
                                    <li><a href="#">One more separated link</a></li>
                                </ul>
                            </li>
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                            <li><a href="#"><i class="fa fa-fw fa-lg fa-user-circle"></i> Connexion</a></li>
                        </ul>
                    </div><!--/.nav-collapse -->
                </div>
            </nav>
        {%endblock%}
        <!-- Begin page content -->
        <div class="container">

            {%block body%}
                <div class="page-header">
                    {%block body_header %}
                        <h1>{{body_title}}</h1>
                    {%endblock%}
                </div>
                <div>
                    {%block body_content%}
                    <div>
                        <p>CONTENU DE LA PAGE</p>
                    </div>
                    <p class="lead">Pin a fixed-height footer to the bottom of the
                        viewport in desktop browsers with this custom HTML and CSS. A fixed
                        navbar has been added with <code>padding-top: 60px;</code> on the <code>body &gt; .container</code>.</p>
                    <p>Back to <a href="https://getbootstrap.com/docs/3.3/examples/sticky-footer">the default sticky footer</a> minus the navbar.</p>
                    <div class="row">
                        <div class="col-sm-3">
                            <form action="index.html" method="get">
                                <select name="all_arbitres" class="form-control list-organize" size="6">
                                    <option value="valeur1">1</option>
                                    <option value="valeur2">2</option>
                                    <option value="valeur3">3</option>
                                    <option value="valeur4">4</option>
                                    <option value="valeur5">5</option>
                                </select>
                                <input type="submit" value="Envoyer" class="btn btn-default" />
                            </form>
                        </div>
                    </div>
                    {%endblock%}
                </div>
            {%endblock%}
        </div>

        {%block footer %}

            <footer class="footer">
                <div class="container">
                    <p class="text-muted">Place sticky footer content here.</p>
                </div>
            </footer>

        {%endblock%}



        <!-- Bootstrap core JavaScript
        ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="{{site_base}}/ressources/js/jquery.js"></script>
        <!--<script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>-->
        <script src="{{site_base}}/ressources/js/bootstrap.min.js"></script>
        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <script src="{{site_base}}/ressources/js/ie10-viewport-bug-workaround.js"></script>

        <script src="{{site_base}}/ressources/js/DDE_js/list_organize.js"></script>
        <script src="{{site_base}}/ressources/js/DDE_js/form_send_all_select.js"></script>
    </body>
</html>